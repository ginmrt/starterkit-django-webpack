{
  "root": true,
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": 1
  }
}
